{
    "name": "studiomitte/basic-authentication",
    "type": "typo3-cms-extension",
    "description": "Implementing basic authentication using PSR-15 middleware",
    "keywords": [
        "TYPO3",
        "extension",
        "basic auth",
        "psr15",
        "middleware"
    ],
    "homepage": "https://www.studiomitte.com",
    "authors": [
        {
            "name": "Georg Ringer",
            "role": "Developer"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "StudioMitte\\BasicAuthentication\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StudioMitte\\BasicAuthentication\\Tests\\": "Tests"
        }
    },
    "replace": {
        "basic_authentication": "self.version",
        "typo3-ter/basic_authentication": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    }
}
